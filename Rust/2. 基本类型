// 基本类型
let i: i32 = 5;      // 32位整数
let u: u32 = 5;      // 32位无符号整数
let f: f64 = 5.0;    // 64位浮点数
let b: bool = true;  // 布尔值
let c: char = 'a';   // 字符
let s: &str = "hello"; // 字符串切片
let string: String = String::from("hello"); // owned 字符串

// 复合类型
let tup: (i32, f64, u8) = (500, 6.4, 1); // 元组
let arr: [i32; 5] = [1, 2, 3, 4, 5];     // 数组
let v: Vec<i32> = Vec::new();            // 向量